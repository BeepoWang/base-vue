{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build --mode production",
    "build:dev": "vite build --mode development",
    "build:test": "vite build --mode test",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "lint:style": "stylelint --fix \"src/**/*.{css,scss,vue,html}\"",
    "format": "prettier --write \"./**/*.{html,vue,ts,js,cjs,json,md}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "vue": "^3.2.41"
  },
  "devDependencies": {
    "@iconify-json/mdi": "^1.1.36",
    "@iconify/json": "^2.1.143",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "@unocss/preset-attributify": "^0.46.5",
    "@unocss/preset-icons": "^0.46.5",
    "@unocss/preset-uno": "^0.46.5",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vueuse/core": "^9.6.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.8.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "pinia": "^2.0.27",
    "postcss": "^8.3.3",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.0",
    "stylelint": "^14.15.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-prettier": "^2.0.0",
    "terser": "^5.16.0",
    "typescript": "^4.6.4",
    "unocss": "^0.46.5",
    "unplugin-auto-import": "^0.12.0",
    "unplugin-icons": "^0.14.14",
    "unplugin-vue-components": "^0.22.11",
    "vite": "^3.2.3",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.0.9"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,vue}": [
      "npm run lint",
      "git add ."
    ],
    "**/*.{css,scss}": [
      "npm run lint:style",
      "git add ."
    ]
  }
}
